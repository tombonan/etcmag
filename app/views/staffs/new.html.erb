<h1>New Staff Signup</h1>


<div class = "blog-form-container">
<%= form_for @staff do |s| %>
	<p>
		<%= s.label :email %><br />
		<%= s.text_field :email %>
	</p>
	<p>
	    <%= s.label :password %><br />
	    <%= s.password_field :password %>
  	</p>
  	<p>
   	 	<%= s.label :password_confirmation %><br />
     	<%= s.password_field :password_confirmation %>
  	</p>
	<p>
		<%= s.submit "Update"%>
	</p>

<% end %>
</div>

